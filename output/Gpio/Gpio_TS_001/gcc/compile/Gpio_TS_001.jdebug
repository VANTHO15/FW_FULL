void AfterTargetHalt (void) {
   
    if(1){
        Util.Log("Jdebug script: AfterTargetHalt is running");
        if(Target.GetReg("R15") == (0x0800097c)) 
        {
            Target.SaveMemory("F:\IAR\FW_THO\FW_THO\FW_HuLa-master\output\Gpio\Gpio_TS_001\gcc\debug\result\dump.log", 0x20000014 - 1, 10); // TestCaseResult[10]
            Debug.Stop();
            File.Exit();
        }
    }
}

